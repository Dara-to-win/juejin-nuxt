{
  "name": "juejin-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=dev BASE_URL=http://localhost:80 set NODE_OPTIONS=--openssl-legacy-provider nuxt --exec babel-node ",
    "build": "cross-env NODE_ENV=pro BASE_URL=https://diandian210.top set NODE_OPTIONS=--openssl-legacy-provider nuxt build ",
    "start": "cross-env NODE_ENV=pro BASE_URL=https://diandian210.top set NODE_OPTIONS=--openssl-legacy-provider nuxt start --exec babel-node ",
    "generate": "cross-env NODE_ENV=pro BASE_URL=https://diandian210.top nuxt generate ",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/proxy": "^2.1.0",
    "babel-cli": "^6.26.0",
    "core-js": "^3.25.3",
    "element-ui": "^2.15.10",
    "less": "^4.1.3",
    "less-loader": "^7",
    "nuxt": "^2.15.8",
    "vue": "^2.7.10",
    "vue-meditor": "^2.1.2",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@nuxtjs/eslint-config": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "prettier": "^2.7.1"
  }
}
